ild1>\n    <!-- 事件总线 -->\n    <Child2 ref=\"child2\" foo=\"foo\" @some-event=\"onSomeEvent\"></Child2>\n  </div>\n</template>\n\n<script>\n  import Child1 from '@/components/communication/Child1.vue'\n  import Child2 from '@/components/communication/Child2.vue'\n  \n  export default {\n    provide() {\n      return {\n        yc: 'yichan'\n      }\n    },\n    components: {\n      Child1, Child2\n    },\n    methods: {\n      onSomeEvent(msg) {\n        console.log('Communition:', msg);\n      }\n    },\n    mounted () {\n      // $children获取子组件数组，不包括普通元素，不保证模板中顺序\n      console.log(this.$children);\n      // $refs用于引用命名的元素或组件，可包含普通元素\n      console.log(this.$refs.child2);\n      \n    },\n  }\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components/communication"}]}