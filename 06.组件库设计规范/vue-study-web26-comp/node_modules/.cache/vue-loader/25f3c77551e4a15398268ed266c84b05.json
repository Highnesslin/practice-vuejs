ort KForm from \"@/components/form/KForm.vue\";\n\n// 没有实现插件，需要导入弹窗和create\n\nexport default {\n  components: {\n    KInput,\n    KFormItem,\n    KForm,\n  },\n  data() {\n    return {\n      model: {\n        username: \"tom\",\n      },\n      rules: {\n        username: [{ required: true, message: \"用户名为必填项\" }],\n      },\n    };\n  },\n  methods: {\n    onLogin() {\n      // 全局校验\n      this.$refs.loginForm.validate((isValid) => {\n        this.$notice({\n          title: \"abc\",\n          message: isValid ? \"过\" : \"不过\",\n        });\n        // if (isValid) {\n        //   console.log(\"submit login\");\n        // } else {\n        //   alert(\"校验失败\");\n        // }\n      });\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}